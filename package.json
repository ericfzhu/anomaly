    "depcheck": "^1.4.3",
    "dotenv": "^10.0.0",
    "framer-motion": "^6.2.8",
    "gatsby": "^4.3.0",
    "gatsby-plugin-image": "^2.9.1",
    "gatsby-plugin-material-ui": "^4.1.0",
