    "gatsby-remark-embed-video": "^3.1.1",
    "gatsby-remark-images": "^6.4.0",
    "gatsby-remark-katex": "^6.3.0",
    "gatsby-source-contentful": "^7.1.0",
    "gatsby-theme-material-ui": "^5.1.0",
    "gatsby-transformer-remark": "^5.3.0",
    "gh-pages": "^3.2.3",
